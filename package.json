    {
      "path": "dist/lg-logger-a.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/aws-lambda": "^8.10.108",
    "@types/lambda-log": "^2.2.1",
    "husky": "^8.0.0",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
